{
  "name": "mario-game",
  "version": "1.0.0",
  "description": "A Mario-like game for Android using Vite and Capacitor",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "cap:add": "npx cap add",
    "cap:sync": "npx cap sync",
    "cap:run": "npx cap run",
    "cap:open": "npx cap open"
  },
  "keywords": [
    "game",
    "mario",
    "android",
    "capacitor",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^1.0.0",
    "jsdom": "^23.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@ionic/core": "^7.0.0",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.5.1"
  }
}
